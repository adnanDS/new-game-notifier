{
    "name": "new-game-notifier",
    "type": "project",
    "license": "MIT",
    "description": "new-game-notifier",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "demigrantsoft/telegram-send-message": "^0.0.1",
        "doctrine/dbal": "^2.10",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.11",
        "phpunit/phpunit": "^8.5",
        "slevomat/coding-standard": "^6.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DemigrantSoft\\Steam\\NewGameNotifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DemigrantSoft\\Steam\\NewGameNotifier\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
